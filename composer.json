{
    "name": "joebeeson/li3_profiler",
    "description": "li3_profiler is a Lithium library for profiling requests built on Jim Rubenstein's PHP Profiler.",
    "type": "lithium-library",
    "authors": [
        {
            "name": "Joe Beeson",
            "email": "jbeeson@gmail.com"
        }
    ],
    "repositories" : {
        "profiler": {
            "type": "package",
            "package": {
                "name": "jimrubenstein/php-profiler",
                "version": "1.0",
                "source": {
                    "url": "git@github.com:jimrubenstein/php-profiler.git",
                    "type": "git",
                    "reference": "HEAD"
                }
            }
        }
    },
    "require": {
        "composer/installers": "*",
        "jimrubenstein/php-profiler": "@dev"
    }
}
